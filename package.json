{
  "expo": {
    "name": "WellzspireApp",
    "slug": "WellzspireApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bhupsai.WellzspireApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bhupsai.WellzspireApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  },
  "dependencies": {
    "axios": "^1.12.2",
    "expo": "^54.0.13",
    "expo-font": "~14.0.9",
    "expo-router": "~6.0.12",
    "expo-splash-screen": "~31.0.10",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-reanimated": "~3.12.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "^0.6.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  }
}
